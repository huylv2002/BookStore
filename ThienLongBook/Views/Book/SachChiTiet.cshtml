    string tenSach = Model.tenSach;
}
@helper Truncate(string s, int length)
{               if (s.Length < length)
    {
        @s
    }
    else
    {
        @s.Substring(0, length)
    }
}

@{
    ViewBag.Title = "Sách chi tiết " + tenSach;
